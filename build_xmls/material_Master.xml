<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Material_Master_Automation_Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />

	 <test name="Test 1">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="add_New_Material_Master"></include>
				</methods>
			</class>
		</classes>
	</test>
	 <test name="Test 2">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="add_New_Material_Master_With_Generate_Description"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Test 3">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="view_Process_Log"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Test 4">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Approve_Status_From_Reviewer"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 5">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Approve_Status_From_Approver"></include>
				</methods>
			</class>
		</classes>
	</test>

	 <test name="Test 6">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Claim_Status"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 7">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Save_As_Draft"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 8">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Change"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 9">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Copy_Record_And_Approve_From_Reviewer"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 10">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Copy_Record_If_User_Remove_One_Child_Hierarchy_Other_Child_Hierarchies_Should_Not_Get_Removed"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 11">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Claim_UnClaim_Status"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 12">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Rejection_Flow_For_New_Record"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 13">
		<classes>
			<class name="Test.MaterialMasterTestcases">
				<methods>
					<include name="verify_Copy_Record_And_Rejection_Flow_For_New_Record"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>
