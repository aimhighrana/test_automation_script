<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="List_Page_Wild_Search_Automation_Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />

	 <test name="Test 1">
		<classes>
			<class name="Test.ListPageWildSearchTestcases">
				<methods>
					<include name="verify_The_Search_Should_Follow_Fuzzy_Logic_Verify_Results_And_Clear_Search"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 2">
		<classes>
			<class name="Test.ListPageWildSearchTestcases">
				<methods>
					<include name="verify_While_Clearing_The_Text_Search_It_Should_Not_Impact_Any_Applied_Filters"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="Test 3">
		<classes>
			<class name="Test.ListPageWildSearchTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Club_The_Text_Search_With_Other_List_Page_Filter"></include>
				</methods>
			</class>
		</classes>
	</test>

</suite>
