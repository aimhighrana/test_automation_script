<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="List Page Wild Search Automation Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAA" />

	 <test name="verify The Search Should Follow Fuzzy Logic Verify Results And Clear Search">
		<classes>
			<class name="Test.ListPageWildSearchTestcases">
				<methods>
					<include name="verify_The_Search_Should_Follow_Fuzzy_Logic_Verify_Results_And_Clear_Search"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify While Clearing The Text Search It Should Not Impact Any Applied Filters">
		<classes>
			<class name="Test.ListPageWildSearchTestcases">
				<methods>
					<include name="verify_While_Clearing_The_Text_Search_It_Should_Not_Impact_Any_Applied_Filters"/>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify The User Should Be Able To Club The Text Search With Other List Page Filter">
		<classes>
			<class name="Test.ListPageWildSearchTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Club_The_Text_Search_With_Other_List_Page_Filter"/>
				</methods>
			</class>
		</classes>
	</test>

</suite>
