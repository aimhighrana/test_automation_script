<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Home Page Automation Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />

	 <test name="verify That In INBOX Folder RecordId Other Data And Functionality Should Be Working Fine">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include
							name="verify_That_In_INBOX_Folder_RecordId_Other_Data_And_Functionality_Should_Be_Working_Fine"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That Different Options Will Be Present Under Action Button">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include name="verify_That_Different_Options_Will_Be_Present_Under_Action_Button"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify Column Details Displayed In Task Folders">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include name="verify_Column_Details_Displayed_In_Task_Folders"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify Failed Records Column For Cross Dataset Integration Daxe Process Log Tasks">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include name="verify_Failed_Records_Column_For_Cross_Dataset_Integration_Daxe_Process_Log_Tasks"/>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify That Once The Record Is Successful ReTrigger For Integration Record Should Not Reflecting In Error Folder">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include
							name="verify_That_Once_The_Record_Is_Successful_ReTrigger_For_Integration_Record_Should_Not_Reflecting_In_Error_Folder"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That All The Filters Type Are Working In Error Folder">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include name="verify_That_All_The_Filters_Type_Are_Working_In_Error_Folder"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That In In progress Folder Record Id Other Data And Functionality Should Be Working Fine">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include
							name="verify_That_In_In_progress_Folder_Record_Id_Other_Data_And_Functionality_Should_Be_Working_Fine"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That In Completed Folder Record Id Other Data Functionality Should Be Working Fine">
		<classes>
			<class name="Test.HomePageTestcases">
				<methods>
					<include
							name="verify_That_In_Completed_Folder_Record_Id_Other_Data_Functionality_Should_Be_Working_Fine"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>
