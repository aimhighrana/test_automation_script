<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="High Level Automation Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAR" />

	 <test name="verifying Material Master Creation WorkFlow 1">
		<classes>
			<class name="Test.HighLevelTestcases">
				<methods>
					<include
							name="verifying_Material_Master_Creation_WorkFlow_1"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verifying Material Creation Default Values WorkFlow 3">
		<classes>
			<class name="Test.HighLevelTestcases">
				<methods>
					<include
							name="verifying_Material_Creation_Default_Values_WorkFlow_3"/>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify change process with System table changes">
		<classes>
			<class name="Test.Workflow11Testcases">
				<methods>
					<include name="verify_change_process_with_System_table_changes"/>
				</methods>
			</class>
		</classes>
	</test>
</suite>
