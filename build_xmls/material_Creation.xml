<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Material Creation Automation Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />

	 <test name="verify That Defaults Are Working In The Transaction Screen According To The User Configured">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_That_Defaults_Are_Working_In_The_Transaction_Screen_According_To_The_User_Configured"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That Created Record Is Visible At Reviewer Task List">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_That_Created_Record_Is_Visible_At_Reviewer_Task_List"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify Data Verification For Reviewer At Transaction Screen">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_Data_Verification_For_Reviewer_At_Transaction_Screen"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify Rejecting The Request From Approver">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_Rejecting_The_Request_From_Approver"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify Approving The Request From Approver">
	<classes>
		<class name="Test.MaterialCreationTestcases">
			<methods>
				<include name="verify_Approving_The_Request_From_Approver"></include>
			</methods>
		</class>
	</classes>
</test>

	<test name="verify Extend Plant Hierarchy From The Created Record Which Is In System Status">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_Extend_Plant_Hierarchy_From_The_Created_Record_Which_Is_In_System_Status"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify Checking Mandatory Fields Error On The Execution Page">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_Checking_Mandatory_Fields_Error_On_The_Execution_Page"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That User Is Able To Save The Record With The Extension Process">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_That_User_Is_Able_To_Save_The_Record_With_The_Extension_Process"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify Reviewer Task List Checks">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_Reviewer_Task_List_Checks"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That Based On The Configured Rule For Description Generator Short And Long Description Should Auto Populate">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_That_Based_On_The_Configured_Rule_For_Description_Generator_Short_And_Long_Description_Should_Auto_Populate"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That After Generating Description Language Grid And PO Text Grid Should Be Auto Populated">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_That_After_Generating_Description_Language_Grid_And_PO_Text_Grid_Should_Be_Auto_Populated"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify Reapproving Rejected Task From Initiator">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_Reapproving_Rejected_Task_From_Initiator"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify ReApproving Rejected Task From Initiator And From Final Approval Step">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="reApproving_Rejected_Task_From_Initiator_And_From_Final_Approval_Step"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify That User Should Be Able To Extend Multiple Similar Child Hierarchies At The Same Time">
		<classes>
			<class name="Test.MaterialCreationTestcases">
				<methods>
					<include name="verify_That_User_Should_Be_Able_To_Extend_Multiple_Similar_Child_Hierarchies_At_The_Same_Time"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>

