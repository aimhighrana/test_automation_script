<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Process Log Automation Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />

	 <test name="verify That User Is Able To View Email Logs When Email Notification Is Configured">
		<classes>
			<class name="Test.ProcessLogTestcases">
				<methods>
					<include name="verify_That_User_Is_Able_To_View_Email_Logs_When_Email_Notification_Is_Configured"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify That User Is Able To View The View Integration Log">
		<classes>
			<class name="Test.ProcessLogTestcases">
				<methods>
					<include name="verify_That_User_Is_Able_To_View_The_View_Integration_Log"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify That Tooltip Is Visible For Comment Icon In Side Sheet">
		<classes>
			<class name="Test.ProcessLogTestcases">
				<methods>
					<include name="verify_That_Tooltip_Is_Visible_For_Comment_Icon_In_Side_Sheet"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify That Tooltip Is Visible For External Icon In Sidesheet">
		<classes>
			<class name="Test.ProcessLogTestcases">
				<methods>
					<include name="verify_That_Tooltip_Is_Visible_For_External_Icon_In_Sidesheet"></include>
				</methods>
			</class>
		</classes>
	</test>
	<test name="verify Classification Data Changes Maintained In The Process Log For Parent Dataset OrRelated Dataset Record">
		<classes>
			<class name="Test.ProcessLogTestcases">
				<methods>
					<include name="verify_Classification_Data_Changes_Maintained_In_The_Process_Log_For_Parent_Dataset_Or_Related_Dataset_Record"></include>
				</methods>
			</class>
		</classes>
	</test>

</suite>
