<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="List_View_Automation_Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />
	 <test name="Test 1">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Add_A_New_View_For_The_List_Page"></include>
				</methods>
			</class>
		</classes>
	</test> 
	 <!-- Test -->

	 <test name="Test 2">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_User_Should_Be_Able_To_Select_Multiple_Records_At_A_Time"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="Test 3">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_User_Should_Be_Able_To_Create_View_Using_Setting_Icon_In_The_List_Page"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="Test 4">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Delete_The_Added_View"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="Test 5">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_Pagination_On_List_Page_Should_Work_As_Expected"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="Test 6">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_View_The_Summary_Of_The_Record_Using_Action_Button_On_The_Records"></include>
				</methods>
			</class>
		</classes>
	</test> 

	<test name="Test 7">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Download_Template_Of_The_Record_Using_Action_Button_On_The_Records"></include>
				</methods>
			</class>
		</classes>
	</test>

	</suite> <!-- Suite -->
