<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="List View Automation Testcases" verbose="10" parallel="tests"
	thread-count="1">
	<listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
    </listeners>
	<parameter name="env" value="QAH" />
	 <test name="verify The User Should Be Able To Add A New View For The List Page">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Add_A_New_View_For_The_List_Page"></include>
				</methods>
			</class>
		</classes>
	</test>

	 <test name="verify User Should Be Able To Select Multiple Records At A Time">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_User_Should_Be_Able_To_Select_Multiple_Records_At_A_Time"></include>
				</methods>
			</class>
		</classes>
	</test>


	<test name="verify User Should Be Able To Create View Using Setting Icon In The List Page">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_User_Should_Be_Able_To_Create_View_Using_Setting_Icon_In_The_List_Page"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify The User Should Be Able To Delete The Added View">
		<classes>
			<class name="Test.ListViewTestcases">
				<methods>
					<include name="verify_The_User_Should_Be_Able_To_Delete_The_Added_View"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="verify The Pagination On List Page Should Work As Expected">
